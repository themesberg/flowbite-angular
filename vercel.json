{
  "version": 2,
  "public": true,
  "name": "flowbite-angular",
  "framework": "angular",
  "buildCommand": "npx nx run docs:build",
  "outputDirectory": "dist/docs-static",
  "installCommand": "npm ci --no-optional",
  "env": {
    "FORCE_RUNTIME_TAG": "canary"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.mjs": {
      "includeFiles": "dist/docs-static/**"
    }
  }
}
