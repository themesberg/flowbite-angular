{
  "version": 2,
  "public": true,
  "name": "flowbite-angular",
  "framework": "angular",
  "buildCommand": "yarn nx run docs:build",
  "outputDirectory": "dist/docs-static",
  "installCommand": "yarn install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.mjs": {
      "includeFiles": "dist/docs-static/browser/**"
    }
  }
}
